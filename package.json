{
  "name": "markdown-it-eleventy-img",
  "description": "A markdown-it plugin that processes images through the eleventy-img plugin. Can be used in any projects that use markdown-it. Fully compatible with the Eleventy static site generator.",
  "version": "0.10.2",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "eslint index.js test.js utilities/*.js && ava",
    "check-updates": "npm outdated"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "ava": "^6.1.2",
    "eslint": "^8.57.0",
    "markdown-it": "^9.0.0",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-implicit-figures": "^0.12.0"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^4.0.2",
    "sharp": "^0.33.2"
  },
  "peerDependencies": {
    "markdown-it": ">= 9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solution-loisir/markdown-it-eleventy-img.git"
  },
  "keywords": [
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "eleventy",
    "11ty",
    "eleventy-utility",
    "eleventy-plugin",
    "eleventy-img"
  ],
  "author": {
    "name": "Mathieu Huot",
    "email": "mathieu.huot@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/solution-loisir/markdown-it-eleventy-img/issues"
  },
  "homepage": "https://github.com/solution-loisir/markdown-it-eleventy-img#readme"
}
